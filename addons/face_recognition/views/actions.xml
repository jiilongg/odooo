<?xml version="1.0" encoding="utf-8"?>
<odoo>
  
    <!-- Student Information -->
    <record id="action_student_information" model="ir.actions.act_window">
        <field name="name">Students</field>
        <field name="res_model">student.information</field>
        <field name="view_mode">list,form</field>
    </record> 

    <record id="action_student_attendance" model="ir.actions.act_window">
        <field name="name">Student Attendance</field>
        <field name="res_model">student.attendance</field>
        <field name="view_mode">list,form</field>
    </record> 

    <record id="study_session_action" model="ir.actions.act_window">
        <field name="name">Study Session</field>
        <field name="res_model">study.session</field>
        <field name="view_mode">list</field> 
    </record>

    <record id="action_face_recognition" model="ir.actions.act_window">
        <field name="name">Face Recognition</field>
        <field name="res_model">face.recognition</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>
 
</odoo>